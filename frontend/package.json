{
  "name": "bio-ton-field-management-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stage": "ng build --configuration 'production, stage'",
    "build:test": "ng build --configuration 'production, test'",
    "build:test2": "ng build --configuration 'production, test2'",
    "build:test3": "ng build --configuration 'production, test3'",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint --max-warnings 0",
    "deploy": "npm run lint && npm run build && npm run test:coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@nextgis/mapboxgl-map-adapter": "^1.16.8",
    "@nextgis/webmap": "^1.16.8",
    "maplibre-gl": "^3.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular-eslint/builder": "15.1.0",
